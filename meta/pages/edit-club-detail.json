  "events": {
    "load": [
      {
        "error": [],
        "name": "conData.getClubDetailByTeamID",
        "parameters": [
          {
            "name": "TeamID",
            "value": "${parameters.TeamID}"
          }
        ],
        "then": [
          {
            "name": "clubdetail",
            "type": "set",
            "value": "${result}",
            "valueType": "SoccerLeagueTransferApp.Models.ConData.ClubDetail"
